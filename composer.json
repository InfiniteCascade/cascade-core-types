{
	"name": "canis/cascade-core-types",
	"description": "Core object types used in Cascade",
	"keywords": ["cascade"],
	"homepage": "http://canis.io",
	"type": "yii2-extension",
	"license": "GPL -2.0",
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"canis/cascade": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*"
	},
	"autoload": {
		"classmap": ["Extension.php"],
		"psr-4": { "cascade\\modules\\core\\": "" }
	},
    "extra": {
    	"bootstrap": "cascade\\modules\\core\\Extension"
    }
}
